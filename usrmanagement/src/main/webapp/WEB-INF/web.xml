<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterProgram</servlet-name>
  	<display-name>RegisterProgram</display-name>
  	<description></description>
  	<servlet-class>usrmanagement.RegisterProgram</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowData</servlet-name>
  	<display-name>ShowData</display-name>
  	<description></description>
  	<servlet-class>usrmanagement.ShowData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditOperation</servlet-name>
  	<display-name>EditOperation</display-name>
  	<description></description>
  	<servlet-class>usrmanagement.EditOperation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteOperation</servlet-name>
  	<display-name>DeleteOperation</display-name>
  	<description></description>
  	<servlet-class>usrmanagement.DeleteOperation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateUserDetails</servlet-name>
  	<display-name>UpdateUserDetails</display-name>
  	<description></description>
  	<servlet-class>usrmanagement.UpdateUserDetails</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterProgram</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowData</servlet-name>
  	<url-pattern>/showdata</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditOperation</servlet-name>
  	<url-pattern>/editurl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteOperation</servlet-name>
  	<url-pattern>/deleteurl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateUserDetails</servlet-name>
  	<url-pattern>/edit</url-pattern>
  </servlet-mapping>
</web-app>
