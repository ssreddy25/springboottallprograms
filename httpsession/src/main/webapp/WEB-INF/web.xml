<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SessionHttp</servlet-name>
  	<display-name>SessionHttp</display-name>
  	<description></description>
  	<servlet-class>sessionhttp.SessionHttp</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SecondPage</servlet-name>
  	<display-name>SecondPage</display-name>
  	<description></description>
  	<servlet-class>sessionhttp.SecondPage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SessionHttp</servlet-name>
  	<url-pattern>/session</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SecondPage</servlet-name>
  	<url-pattern>/second</url-pattern>
  </servlet-mapping>
</web-app>
