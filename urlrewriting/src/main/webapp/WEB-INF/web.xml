<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UrlFristClass</servlet-name>
  	<display-name>UrlFristClass</display-name>
  	<description></description>
  	<servlet-class>urlwrtingprograms.UrlFristClass</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SecondUrl</servlet-name>
  	<display-name>SecondUrl</display-name>
  	<description></description>
  	<servlet-class>urlwrtingprograms.SecondUrl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UrlFristClass</servlet-name>
  	<url-pattern>/url</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SecondUrl</servlet-name>
  	<url-pattern>/second</url-pattern>
  </servlet-mapping>
</web-app>
